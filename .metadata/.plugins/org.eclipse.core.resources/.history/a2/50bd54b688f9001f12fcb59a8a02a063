package com.app.dao;

import org.apache.ibatis.session.SqlSession;

import com.app.mybatis.config.MyBatisConfig;

public class ProductDAO {
	public SqlSession sqlSession;
	
	public ProductDAO() {
		MyBatisConfig.getSqlSessionFactory().openSession(true).insert(null);
	}
	
//	상품 추가
	public void insert() {
		sqlSession.insert("product.insert");
	}
}
